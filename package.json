{
	"name": "page-speed-insights",
	"version": "0.0.1",
	"private": true,
	"description": "GitHub Action to validate a site with Page Speed Insights",
	"author": "Jeppe Reinhold <jeppe@reinhold.is> (https://reinhold.is)",
	"license": "MIT",
	"packageManager": "pnpm@7.5.2",
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint ./**/*.ts",
		"lint:fix": "eslint ./**/*.ts --fix",
		"test": "vitest",
		"type-check": "tsc --noEmit",
		"validate": "pnpm format:check && pnpm lint && pnpm test && pnpm build && pnpm build",
		"build": "tsup ./src/index.ts",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JReinhold/page-speed-insights.git"
	},
	"keywords": [
		"actions",
		"page speed insights",
		"psi"
	],
	"devDependencies": {
		"@actions/core": "^1.6.0",
		"@tsconfig/node16-strictest-esm": "^1.0.3",
		"@types/node": "^18.0.6",
		"@typescript-eslint/parser": "^5.8.1",
		"@vercel/ncc": "^0.34.0",
		"eslint": "^8.20.0",
		"eslint-plugin-github": "^4.3.2",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.0",
		"js-yaml": "^4.1.0",
		"prettier": "2.7.1",
		"pretty-quick": "^3.1.3",
		"tsup": "^6.1.3",
		"typescript": "^4.4.4",
		"vitest": "^0.18.1"
	}
}
